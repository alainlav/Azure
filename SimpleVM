
New-AzureRmVM `
  -ResourceGroupName $RGname `
  -Name 'TestalainVM' `
  -Location 'TestAlain' `
  -ImageName "Win2016Datacenter" `
  -VirtualNetworkName "myVnet" `
  -SubnetName "mySubnet" `
  -SecurityGroupName "myNetworkSecurityGroup" `
  -PublicIpAddressName "myPublicIp" `
  -Credential $cred `
  -OpenPorts 3389
